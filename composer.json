{
    "name": "partoo/laravel-widgets",
    "description": "A repo forked from arrilot/laravel-widgets",
    "license": "MIT",
    "homepage": "https://github.com/Partoo/laravel-widgets",
    "require": {
        "php": ">=7.0",
        "illuminate/support": ">=5.5",
        "illuminate/contracts": ">=5.5",
        "illuminate/view": ">=5.5",
        "illuminate/container": ">=5.5",
        "illuminate/console": ">=5.5",
        "illuminate/cache": ">=5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Partoo\\Widgets\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Partoo\\Widgets\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Partoo\\Widgets\\ServiceProvider"
            ],
            "aliases": {
                "Widget": "Partoo\\Widgets\\Facade",
                "AsyncWidget": "Partoo\\Widgets\\AsyncFacade"
            }
        }
    }
}
